{
  "type": "module",
  "engines": {
    "node": ">=20.19.2"
  },
  "name": "prueba",
  "version": "1.0.0",
  "description": "Proyecto TypeScript con TypeORM y Serverless",
  "main": "dist/server/local.js",
  "scripts": {
    "build:layers:redis": "tsc -p layers/redis/tsconfig.json",
    "build": "tsc && pnpm build:layers:redis",
    "migration:create": "cross-env NODE_OPTIONS=--require=dotenv/config pnpm typeorm migration:create",
    "migration:generate": "cross-env NODE_OPTIONS=--require=dotenv/config ts-node-esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js -- migration:generate -d ./src/data-source.cli.ts",
    "migration:run": "cross-env NODE_OPTIONS=--require=dotenv/config ts-node-esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js -- migration:run -d ./src/data-source.cli.ts",
    "migration:revert": "cross-env NODE_OPTIONS=--require=dotenv/config ts-node-esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js -- migration:revert -d ./src/data-source.cli.ts",
    "deploy": "cross-env NODE_OPTIONS=--experimental-vm-modules sls deploy",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:cov": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:redis": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/redis-layer.test.ts",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "dev": "cross-env NODE_OPTIONS=--require=dotenv/config tsx src/server/local.ts",
    "offline": "cross-env NODE_OPTIONS=--require=dotenv/config sls offline start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@types/swagger-ui-express": "^4.1.8",
    "aws-lambda": "^1.0.7",
    "express": "^4.19.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "reflect-metadata": "^0.2.2",
    "serverless": "^4.17.0",
    "serverless-offline": "^14.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@serverless/typescript": "^4.15.1",
    "@types/aws-lambda": "^8.10.149",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "jest": "^30.0.0",
    "openapi-types": "^12.1.3",
    "serverless-plugin-layer-manager": "^1.1.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typeorm-extension": "^3.7.1",
    "typescript": "^5.8.3"
  }
}